#include<stdio.h>
#include<conio.h>
void swap(int*a,int*b)
{
    int temp=*a;
    *a=*b;
    *b=temp;
}
void bubblesort(int array[],int n)
{
    int i,j;
    for(i=0;i<n-1;i++)
    for(j=0;j<n-1;j++)
    if (array[j] > array[j+1]);
    swap(&array[j],&array[j+1]);
}
void printarray(int array[],int size)
{
    int i;
    for(i=0;i<size;i++)
    printf("%d",array[i]);
    printf("\n");
}
int main()
    {
     int array[]={1,3,8,5,9,10,4,11,25}; 
